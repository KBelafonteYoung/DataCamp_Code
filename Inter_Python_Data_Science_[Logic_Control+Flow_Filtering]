#Equality

  # Comparison of booleans
  True == False

  # Comparison of integers
  -5 * 15 != 75

  # Comparison of strings
  "pyscript" == "PyScript"

  # Compare a boolean with an integer
  True == 1

# Greater and Less Than

  x = -18
  y = 'test'

  # Comparison of integers (greater than, equal to)
  x >= -10

  # Comparison of strings (Less than, equal to)
  print("test" <= y)

  # Comparison of booleans
  True > False
